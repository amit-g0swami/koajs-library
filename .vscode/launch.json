  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "koa-client",
      "args": ["${workspaceFolder}/src/sample.ts"],
      "runtimeArgs": ["-r", "ts-node/register"],
      "env": {
        "TS_NODE_PROJECT": "${workspaceFolder}/tsconfig.json"
      },
      "outputCapture": "std",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
